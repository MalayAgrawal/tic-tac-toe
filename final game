				{
					a[2][2]='O';
					goto win;
				}
				else
				{
					a[2][1]='O';
					goto win;
				}
			}
		}
		if((a[1][2]=='X')&&(a[2][1]=='X'))
		{
			a[2][2]='O';
			input();
			if(a[0][0]!='X')
			{
				a[0][0]='O';
				goto win;
			}
			else
			{
				a[2][0]='O';
				input();
				if(a[0][2]!='X')
				{
					a[0][2]='O';
					goto win;
				}
				else
				{
					a[0][1]='O';
					goto win;
				}
			}
		}
		if((a[2][1]=='X')&&(a[1][0]=='X'))
		{
			a[2][0]='O';
			input();
			if(a[0][2]!='X')
			{
				a[0][2]='O';
				goto win;
			}
			else
			{
				a[2][2]='O';
				input();
				if(a[0][0]!='X')
				{
					a[0][0]='O';
					goto win;
				}
				else
				{
					a[0][1]='O';
					goto win;
				}
			}
		}
		if((a[0][1]=='X')&&(a[2][1]=='X'))
		{
			a[0][0]='O';
			input();
			if(a[2][2]!='X')
			{
				a[2][2]='O';
				goto win;
			}
			else
			{
				a[2][0]='O';
				input();
				if(a[1][0]!='X')
				{
					a[1][0]='O';
					goto win;
				}
				else
				{
					a[0][2]='O';
					goto win;
				}
			}
		}
		if((a[1][2]=='X')&&(a[1][0]=='X'))
		{
			a[0][0]='O';
			input();
			if(a[2][2]!='X')
			{
				a[2][2]='O';
				goto win;
			}
			else
			{
				a[0][2]='O';
				input();
				if(a[0][1]!='X')
				{
					a[0][1]='O';
					goto win;
				}
				else
				{
					a[2][0]='O';
					goto win;
				}
			}
		}			
   }
win:
{
pp();
victory();
}
}
void input()
{
	int p;
	pp();
	printf("\n enetr the squre number = ");
	scanf("%d",&p);
	switch(p)
		{
			case 11:
			a[0][0]='X';
			break;
			case 12:
			a[0][1]='X';
			break;
			case 13:
			a[0][2]='X';
			break;
			case 21:
			a[1][0]='X';
			break;
			case 22:
			a[1][1]='X';
			break;
			case 23:
			a[1][2]='X';
			break;
			case 31:
			a[2][0]='X';
			break;
			case 32:
			a[2][1]='X';
			break;
			case 33:
			a[2][2]='X';
			break;
		}
	pp();
}
void victory()
{
pp();
printf("computer wins");
}
void pp()
{
	clrscr();
printf("\n\n");
printf("      %c |  %c  |   %c             11  |  12  | 13 \n",a[0][0],a[0][1],a[0][2]);
printf("    ----|-----|-----           -----|------|---- \n");
printf("      %c |  %c  |   %c             21  |  22  | 23\n",a[1][0],a[1][1],a[1][2]);
printf("    ----|-----|-----           -----|------|---- \n");
printf("      %c |  %c  |   %c             31  |  32  | 33\n\n",a[2][0],a[2][1],a[2][2]);
}
